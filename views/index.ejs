<h1>Home Page</h1>

<div class="container">
    <div class="row row-cols-4">
        
        <% players.forEach((player)=>{ %>
            <div class="col-4"></div>
                <h3><%= player.name %></h3> 
                <img src="<%= player.coverImagePath %>" alt="<%= player.name %>">
            </div>
        <% }) %>
        
    </div>
</div>

 